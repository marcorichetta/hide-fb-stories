{
	"manifest_version": 2,
	"name": "Hide Facebook stories",
	"description": "This extensions hides stories on Facebook",
	"version": "0.0.1",
	"developer": {
		"name": "Marco Richetta",
		"url": "https://marcorichetta.now.sh"
	},
	"content_scripts": [
		{
			"matches": ["*://*.facebook.com/*"],
			"css": ["hide.css"]
		}
	],
	"icons": {
		"48": "icons/fb.png",
		"96": "icons/fb96.png"
	}
}
